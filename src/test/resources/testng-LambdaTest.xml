<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="MySuite" thread-count="3" parallel="tests">
    <test name="ChromeTest">
        <parameter name="browserName" value="Chrome"></parameter>
        <parameter name="browserVersion" value="131"></parameter>
        <parameter name="osVersion" value="Windows 11"></parameter>
        <classes>
            <class name="Test.WikipideaSearchTest"/>
        </classes>
    </test>

    <test name="FireFoxTest">
        <parameter name="browserName" value="Firefox"></parameter>
        <parameter name="browserVersion" value="132"></parameter>
        <parameter name="osVersion" value="Windows 11"></parameter>
        <classes>
            <class name="Test.WikipideaSearchTest"/>
        </classes>
    </test>

    <test name="EdgeTest">
        <parameter name="browserName" value="Edge"></parameter>
        <parameter name="browserVersion" value="131"></parameter>
        <parameter name="osVersion" value="Windows 11"></parameter>
        <classes>
            <class name="Test.WikipideaSearchTest"/>
        </classes>
    </test>
</suite>